{
  "version": 2,
  "buildCommand": "tsc --project tsconfig.json",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "dist/server.js"
    }
  ]
}
